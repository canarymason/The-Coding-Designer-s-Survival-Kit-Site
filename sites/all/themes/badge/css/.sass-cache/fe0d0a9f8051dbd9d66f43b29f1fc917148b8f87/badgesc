// All sprites should extend this class
// The badges-sprite mixin will do so for you.
#{$badges-sprite-base-class} {
  background: $badges-sprites no-repeat;
}

// Use this to set the dimensions of an element
// based on the size of the original image.
@mixin badges-sprite-dimensions($name) {
  @include sprite-dimensions($badges-sprites, $name)
}

// Move the background position to display the sprite.
@mixin badges-sprite-position($name, $offset-x: 0, $offset-y: 0) {
  @include sprite-background-position($badges-sprites, $name, $offset-x, $offset-y)
}

// Extends the sprite base class and set the background position for the desired sprite.
// It will also apply the image dimensions if $dimensions is true.
@mixin badges-sprite($name, $dimensions: $badges-sprite-dimensions, $offset-x: 0, $offset-y: 0) {
  @extend #{$badges-sprite-base-class};
  @include sprite($badges-sprites, $name, $dimensions, $offset-x, $offset-y)
}

@mixin badges-sprites($sprite-names, $dimensions: $badges-sprite-dimensions, $prefix: sprite-map-name($badges-sprites)) {
  @include sprites($badges-sprites, $sprite-names, $badges-sprite-base-class, $dimensions, $prefix)
}

// Generates a class for each sprited image.
@mixin all-badges-sprites($dimensions: $badges-sprite-dimensions, $prefix: sprite-map-name($badges-sprites)) {
  @include badges-sprites(clean-start-selected clean-start code-viewer-selected code-viewer color-selected color drupal-ui-patterns-selected drupal-ui-patterns fonts-selected fonts html5-selected html5 jquery-plugins-selected jquery-plugins sass-mixins-selected sass-mixins step-by-step-selected step-by-step ui-patterns-selected ui-patterns, $dimensions, $prefix);
}
